    "#Deadline Updates\n",
    "import pandas as pd\n",
    "from datetime import date, timedelta\n",
    "import os\n",
    "import numpy as np\n",
    "from hdbcli import dbapi\n",
    "\n",
    "\n",
    "filepath = r'<<FILEPATH>>.xlsx'\n",
    "dl = pd.read_excel(filepath, usecols=range(0,5), index=False)\n",
    "dl = dl.dropna()\n",
    "dl = dl.astype({'ManagedBy':'int64'})\n",
    "\n",
    "dl['ManagedBy']=dl.apply(lambda x: 'MB420000000'+str(x['ManagedBy']) if len(str(x['ManagedBy']))==3 else \n",
    "                         ('MB4200000000'+str(x['ManagedBy']) if len(str(x['ManagedBy']))==2 else \n",
    "                          ('MB42000000000'+str(x['ManagedBy']) if len(str(x['ManagedBy']))==1 else \n",
    "                           ('MB42000000'+str(x['ManagedBy']) if len(str(x['ManagedBy']))==4 else x))), axis=1)\n",
    "\n",
    "dl=dl.astype({'Timestamp':'str'})\n",
    "xstamp=dl['Timestamp'].str.split(' ', n=1, expand=True)\n",
    "dl['Time']=xstamp[1]\n",
    "dl['Date']=xstamp[0]\n",
    "dl=dl.astype({'Date':'datetime64[ns]'})\n",
    "dl['Date']=dl['Date'].dt.strftime('%m/%d/%Y')\n",
    "\n",
    "#Connect to HANA\n",
    "conn = dbapi.connect(key='hanakey',CONNECTTIMEOUT=10000)\n",
    "print('Connected!')\n",
    "cursor=conn.cursor()\n",
    "\n",
    "#Drop Table\n",
    "cursor.executemany('''Drop Table \"<<SQL TABLE PATH>>";''', ())\n",
    "print('Table Dropped')\n",
    "\n",
    "#Re-Create Table\n",
    "cursor.executemany('''create column table \"<<SQL TABLE PATH>>" (\<<TABLE FIELDS/VARIABLES>>;''', ())\n",
    "print('Table Created')\n",
    "subset = dl[[<<FIELD NAMES>>]]\n",
    "subset.replace(np.NaN, 'None')\n",
    "tuples = [tuple(x) for x in subset.to_numpy()]\n",
    "\n",
    "#Insert Data\n",
    "cursor.executemany('''INSERT INTO \"<<SQL TABLE PATH>>" (\<<TABLE FIELDS/VARIABLES>>) Values(?,?,?,?,?,?,?)''', (tuples))\n",
    "print('Data Inserted')\n",
    "conn.commit()"
   ]
  },
