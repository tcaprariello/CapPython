    "no['IssueNotesNP'] = no['IssueNotesNP'].apply(lambda x: re.sub(r\"[^a-zA-Z0-9 .]+\", '',str(x)))\n",
    "no['ActionRequiredNP'] = no['ActionRequiredNP'].apply(lambda x: re.sub(r\"[^a-zA-Z0-9 .]+\", '',str(x)))\n",
    "no['ActionAssignedToNP'] = no['ActionAssignedToNP'].apply(lambda x: re.sub(r\"[^a-zA-Z0-9 .]+\", '',str(x)))\n",
    "no['MonthNP'] = no['MonthNP'].apply(lambda x: re.sub(r\"[^a-zA-Z0-9 .]+\", '',str(x)))\n",
    "no['IssueNotesNP']=no['IssueNotesNP'].str[:245]\n",
    "\n",
    "#Connect to HANA\n",
    "conn = dbapi.connect(key='hanakey',CONNECTTIMEOUT=10000)\n",
    "print('Connected!')\n",
    "cursor=conn.cursor()\n",
    "\n",
    "#Drop\n",
    "#cursor.executemany('''Drop Table \"<<TABLE FIELDS/VARIABLES>>";''', ())\n",
    "#print('Table Dropped')\n",
    "\n",
    "#Create\n",
    "#cursor.executemany('''create column table \"<<TABLE FIELDS/VARIABLES>>" (\"ManagedBy\" VARCHAR (25) NULL, \"Month\" VARCHAR (15) NULL,\n",
    "#                    \"ActionRequired\" VARCHAR (250) NULL, \"ActionAssignedTo\" VARCHAR (100) NULL, \n",
    "#                    \"IssueNotes\" VARCHAR (260) NULL,  \"Timestamp\" VARCHAR (30) NULL, \"CreatedBy\" VARCHAR (50) NULL, \n",
    "#                    \"Time\" VARCHAR (15) NULL, \"Date\" DATE NULL);''', ())\n",
    "#print('Table Created')\n",
    "\n",
    "#Insert\n",
    "subset = no[['ManagedByNP','MonthNP','ActionRequiredNP','ActionAssignedToNP','IssueNotesNP',\n",
    "             'TimestampNP','CreatedByNP','TimeNP','DateNP']]\n",
    "subset.replace(np.NaN, 'None')\n",
    "tuples = [tuple(x) for x in subset.to_numpy()]\n",
    "\n",
    "cursor.executemany('''INSERT INTO \"<<SQLTABLENAME>>" (\"ManagedBy\",\"Month\",\"ActionRequired\",\"ActionAssignedTo\",\n",
    "                    \"IssueNotes\",\"Timestamp\",\"CreatedBy\",\"Time\",\"Date\") Values(?,?,?,?,?,?,?,?,TO_DATE(?,'MM/DD/YYYY'))''', (tuples))\n",
    "\n",
    "print('Data Inserted')\n",
    "\n",
    "#Remove Dup Rows\n",
    "cursor.executemany('''delete from \"<<SQLTABLENAME>>"\n",
    "where \"$rowid$\" in\n",
    "(select lead(\"$rowid$\") over (partition by \"ManagedBy\",\"IssueNotes\",\"Date\" order by \"ManagedBy\",\"IssueNotes\") from \"CCC_SELF_SERVE\".\"FSS_NP_STATUS\")''')\n",
    "print('Dups Dropped')\n",
    "\n",
    "conn.commit()"
   ]
