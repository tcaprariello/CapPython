    "no=no.astype({'TimestampNP':'str'})\n",
    "xstamp=no['TimestampNP'].str.split(' ', n=1, expand=True)\n",
    "no['TimeNP']=xstamp[1]\n",
    "no['DateNP']=xstamp[0]\n",
    "\n",
    "no=no.astype({'DateNP':'datetime64[ns]'})\n",
    "no['DateNP']=no['DateNP'].dt.strftime('%m/%d/%Y') \n",
